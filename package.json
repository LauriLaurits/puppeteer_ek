{
  "name": "puppeteer_ek",
  "version": "1.0.0",
  "description": "Automated testing for EK",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles ./test/example.test.js",
    "test:device": "jest --detectOpenHandles ./test/device.test.js",
    "test:snapshot": "jest --config=jest.config.js --detectOpenHandles --forceExit",
    "test:snapshot:update": "jest --config=jest.config.js --updateSnapshot --detectOpenHandles --forceExit"
  },
  "author": "LauriL",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.2.0",
    "jest": "^25.1.0",
    "jest-image-snapshot": "^2.12.0",
    "puppeteer": "^2.1.1"
  },
  "devDependencies": {
    "mocha": "^10.2.0"
  }
}
